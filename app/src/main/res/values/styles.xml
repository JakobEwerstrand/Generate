<resources>

    <!-- Base application theme. -->
    <style workoutName="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item workoutName="colorPrimary">@color/colorPrimary</item>
        <item workoutName="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item workoutName="colorAccent">@color/colorAccent</item>
    </style>

    <style workoutName="AppTheme.NoActionBar">
        <item workoutName="windowActionBar">false</item>
        <item workoutName="windowNoTitle">true</item>
    </style>

    <style workoutName="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style workoutName="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
